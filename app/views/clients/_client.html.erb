<div id="<%= dom_id(client) %>">
  <p>
    <strong><%= t(".name") %></strong>
    <%= client.name %>
  </p>
  <p>
    <strong><%= t(".firstname") %></strong>
    <%= client.firstname %>
  </p>
  <p>
    <strong><%= t(".email") %></strong>
    <%= client.email %>
  </p>
  <p>
    <strong><%= t(".phone") %></strong>
    <%= client.phone %>
  </p>
  <p>
    <strong><%= t(".address") %></strong>
    <%= client.address %>
  </p>
  <p>
    <strong><%= t(".zip_code") %></strong>
    <%= client.zip_code %>
  </p>
  <p>
    <strong><%= t(".city") %></strong>
    <%= client.city %>
  </p>
  <p>
    <strong><%= t(".country") %></strong>
    <%= client.country %>
  </p>
  <p>
    <strong><%= t(".birthdate") %></strong>
    <%= client.birthdate %>
  </p>
  <p>
    <strong><%= t(".how_do_you_know_us") %></strong>
    <%= client.how_do_you_know_us %>
  </p>
  <p>
    <strong><%= t(".notes") %></strong>
    <%= client.notes %>
  </p>
  <% if client.lash %>
    <h3><%= t("lash_global_info") %></h3>
    <p>
      <strong><%= t(".lash_desired_effect") %></strong>
      <%= client.lash.desired_effect %>
    </p>
    <p>
      <strong><%= t(".lash_face_diagnostic") %></strong>
      <%= client.lash.face_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_asymmetry_diagnostic") %></strong>
      <%= client.lash.asymmetry_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_eyelid_morphology_diagnostic") %></strong>
      <%= client.lash.eyelid_morphology_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_alignment_morphology_diagnostic") %></strong>
      <%= client.lash.alignment_morphology_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_proportion_morphology_diagnostic") %></strong>
      <%= client.lash.proportion_morphology_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_thickness_diagnostic") %></strong>
      <%= client.lash.thickness_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_length_diagnostic") %></strong>
      <%= client.lash.length_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_curvature_diagnostic") %></strong>
      <%= client.lash.curvature_diagnostic %>
    </p>
    <p>
      <strong><%= t(".lash_style") %></strong>
      <%= client.lash.style %>
    </p>
    <p>
      <strong><%= t(".lash_notes") %></strong>
      <%= client.lash.notes %>
    </p>
    <p>
      <strong><%= t(".lash_mapping") %></strong>
      <%= client.lash.mapping %>
    </p>
    <p>
      <strong><%= t(".lash_event") %></strong>
      <%= client.lash.event %>
    </p>
    <p>
      <strong><%= t(".lash_texture") %></strong>
      <%= client.lash.texture %>
    </p>
    <p>
      <strong><%= t(".lash_density") %></strong>
      <%= client.lash.density %>
    </p>
  <% else %>
    <p><em><%= t(".no_lash_information") %></em></p>
  <% end %>
  <% if client.extension %>
    <h3> <strong><%= t(".lash_extension_info") %></strong></h3>
    <p>
      <strong><%= t(".lash_extension_brand") %></strong>
      <%= client.extension.brand %>
    </p>
    <p>
      <strong><%= t(".lash_extension_curvature") %></strong>
      <%= client.extension.curvature %>
    </p>
    <p>
      <strong><%= t(".lash_extension_thickness") %></strong>
      <%= client.extension.thickness %>
    </p>
    <p>
      <strong><%= t(".lash_extension_glue") %></strong>
      <%= client.extension.glue %>
    </p>
  <% else %>
    <p><em><%= t(".no_lash_extension_information") %></em></p>
  <% end %>
  <% if client.health %>
    <h3><%= t(".health_info") %></h3>
    <p>
      <strong><%= t(".health_diabetes") %></strong>
      <%= client.health.diabetes %>
    </p>
    <p>
      <strong><%= t(".health_pregnancy") %></strong>
      <%= client.health.pregnancy %>
    </p>
    <p>
      <strong><%= t(".health_dry_eyes") %></strong>
      <%= client.health.dry_eyes %>
    </p>
    <p>
      <strong><%= t(".health_teary_eyes") %></strong>
      <%= client.health.teary_eyes %>
    </p>
    <p>
      <strong><%= t(".health_allergy") %></strong>
      <%= client.health.allergy %>
    </p>
    <p>
      <strong><%= t(".health_contact_lenses") %></strong>
      <%= client.health.contact_lenses %>
    </p>
    <p>
      <strong><%= t(".health_surgery") %></strong>
      <%= client.health.surgery %>
    </p>
    <p>
      <strong><%= t(".health_chemotherapy") %></strong>
      <%= client.health.chemotherapy %>
    </p>
    <p>
      <strong><%= t(".health_eyes_allergy") %></strong>
      <%= client.health.eyes_allergy %>
    </p>
    <p>
      <strong><%= t(".health_itch") %></strong>
      <%= client.health.itch %>
    </p>
    <p>
      <strong><%= t(".health_first_application") %></strong>
      <%= client.health.first_application %>
