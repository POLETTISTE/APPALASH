<div data-search-guests-target="list ">
  <% if params[:query].present? %>
    <p class="query-guests">
      <%= t('.list_with_query_html', count: @guests.size, query: params[:query]) %>
    </p>
  <% end %>
  <% @guests.each do |guest| %>
    <%= render "guests/index/guest_card", guest: guest %>
  <% end %>
</div>
